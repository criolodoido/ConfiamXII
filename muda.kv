<PrimeiroScreen>:
	canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'tela.jpg'
	StackLayout:
		orientation: 'bt-lr'
		GridLayout:
			rows: 1
			padding: 0, 0
			spacing: 0, 0
			size_hint: (1, .3)
			Button:
				text: "Programação\n   Inscrição"
				bold: True
				font_size: '13.5dp'			
				on_release: root.manager.current = 'dois'
				background_normal: 'btn1normal.png'
				background_down: 'btn1down.png'
			
			Button:
				text: "Palestrantes"
				font_size: '13.5dp'
				bold: True
				on_release: root.manager.current = 'pa'
				background_normal: 'btn1normal.png'
				background_down: 'btn1down.png'
			Button:
				text: "Mini-cursos\nMesa Redonda"
				font_size: '13.5dp'
				bold: True
				background_normal: 'btn1normal.png'
				background_down: 'btn1down.png'
				on_release: root.manager.current = 'mc'
				
			
